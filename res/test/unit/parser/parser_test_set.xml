    </schema>
  </test>
  <test group="attribute" name="schema::finalDefault #all extension restriction list union">
    <schema dir="schema" fileName="attr_finald_0009.xsd">
      <exception>\PhpXmlSchema\Exception\InvalidValueException</exception>
      <message>"#all extension restriction list union" is an invalid value for the "finalDefault" attribute (from no namespace), expected: "#all" or "List of (extension | restriction | list | union)".</message>
    </schema>
  </test>
  <!-- schema::id -->
  <test group="attribute" name="schema::id">
    <schema dir="schema" fileName="attr_id_0002.xsd">
      <exception>\PhpXmlSchema\Exception\InvalidValueException</exception>
      <message>"foo:bar" is an invalid ID.</message>
