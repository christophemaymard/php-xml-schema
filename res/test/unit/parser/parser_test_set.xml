  <!-- schema::xml::lang -->
  <test group="attribute" name="schema::xml::lang (empty string)">
    <schema dir="schema" fileName="attr_lang_0002.xsd">
      <exception>\PhpXmlSchema\Exception\InvalidValueException</exception>
      <message>"" is an invalid primary subtag.</message>
