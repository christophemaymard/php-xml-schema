      <exception>\PhpXmlSchema\Exception\InvalidValueException</exception>
      <message>"foo" is an invalid value for the "finalDefault" attribute (from no namespace), expected: "#all" or "List of (extension | restriction | list | union)".</message>
    </schema>
  </test>
  <test group="attribute" name="schema::finalDefault #ALL">
    <schema dir="schema" fileName="attr_finald_0007.xsd">
      <exception>\PhpXmlSchema\Exception\InvalidValueException</exception>
